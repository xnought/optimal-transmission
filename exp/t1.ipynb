   "source": [
    "def log_history(history, msg):\n",
    "\tfor char in msg:\n",
    "\t\tif char in history:\n",
    "\t\t\thistory[char] += 1\n",
    "\t\telse:\n",
    "\t\t\thistory[char] = 1\n",
    "\t\n",
    "def send_msg(msg):\n",
    "\tlog_history(history, msg)\n",
    "\treturn bytes(msg.encode())"
   ]
  },
